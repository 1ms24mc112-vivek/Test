pipeline {
    agent any
    
    stages{
      stage('Checkout'){
        steps {
            git branch: 'main', url: 'https://github.com/1ms24mc112-vivek/Test.git'
             }
        }
    stage('Run'){
        steps {
            echo "Running Python Program..."
            bat 'python3 python.py'
          }
        }
    }
}
