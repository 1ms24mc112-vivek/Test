pipeline {
    agent any
    trigger {
        CRON('* * * * *')
    }
    stages{
      stage('Checkout'){
        steps {
            git branch: 'main', url: 'https://github.com/1ms24mc112-vivek/Test.git'
             }
        }
    stage('Python'){
        steps {
            sh 'python3 python.py'            
            }
        }
    stage('Test'){
        steps {
            echo "Testing PYTHON FILE........."        
             }
        }
    }
}
